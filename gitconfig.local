[user]
  name = Nick Dainty
  email = nickdainty@gmail.com
[push]
  default = upstream
[alias]
  aa = add --all
  d = diff
  dc = diff --cached
  cleanup = !git remote prune origin && git gc && git clean -dfx && git stash clear
  new-feature = !sh -c 'git checkout -b $1 && git push -u origin $1' -
  test-feature = !sh -c 'git checkout develop && git pull && git merge --ff --no-edit $1 && git push' -
  checkout-feature = !sh -c 'git checkout --track -b $1 origin/$1' -
