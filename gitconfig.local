[user]
  name = Nick Dainty
  email = nickdainty@gmail.com
[push]
  default = upstream
[alias]
  aa = add --all
  d = diff
  dc = diff --cached
  checkout-remote-branch = !sh -c 'git checkout --track -b $1 origin/$1' -
  cleanup = !git remote prune origin && git gc && git clean -dfx && git stash clear
[init]
  templatedir = ~/.git_template
